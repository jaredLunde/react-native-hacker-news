{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/assets/*": ["assets/*"],
      "@/components/*": ["components/*"],
      "@/dash": ["dash.config"],
      "@/hooks/*": ["hooks/*"],
      "@/screens/*": ["screens/*"],
      "@/styles/*": ["styles/*"],
      "@/stores/*": ["stores/*"],
